<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Alsbtain.EastPha" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>EastPha</name>
    <description>
        EastPha.com
    </description>
    <author email="info@alsbtain.net" href="http://alsbtain.net">
        Alsbtain Development Team
    </author>
	<preference name="phonegap-version" value="cli-7.1.0" />
	<plugin name="cordova-plugin-inappbrowser" spec="2.0.2" />
	<plugin name="cordova-plugin-network-information" spec="2.0.1" />
	<plugin name="cordova-plugin-whitelist" spec="1.3.3" />
	<plugin name="cordova-plugin-splashscreen" source="npm" />
	<platform name="android">
        <!--
            ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px
        -->
        <icon src="www/res/android/icon/ldpi.png" density="ldpi" />
        <icon src="www/res/android/icon/mdpi.png" density="mdpi" />
        <icon src="www/res/android/icon/hdpi.png" density="hdpi" />
        <icon src="www/res/android/icon/xhdpi.png" density="xhdpi" />
        <icon src="www/res/android/icon/xxhdpi.png" density="xxhdpi" />
        <icon src="www/res/android/icon/xxxhdpi.png" density="xxxhdpi" />
    </platform>
	<platform name="android">
		<!-- you can use any density that exists in the Android project -->
		<splash src="www/res/android/screen/800X480.png" density="land-hdpi"/>
		<splash src="www/res/android/screen/320X200.png" density="land-ldpi"/>
		<splash src="www/res/android/screen/480X320.png" density="land-mdpi"/>
		<splash src="www/res/android/screen/1280X720.png" density="land-xhdpi"/>
		<splash src="www/res/android/screen/1600X960.png" density="land-xxhdpi"/>
		<splash src="www/res/android/screen/1920X1280.png" density="land-xxxhdpi"/>
		<splash src="www/res/android/screen/480X800.png" density="port-hdpi"/>
		<splash src="www/res/android/screen/200X320.png" density="port-ldpi"/>
		<splash src="www/res/android/screen/320X480.png" density="port-mdpi"/>
		<splash src="www/res/android/screen/720X1280.png" density="port-xhdpi"/>
		<splash src="www/res/android/screen/960X1600.png" density="port-xxhdpi"/>
		<splash src="www/res/android/screen/1280X1920.png" density="port-xxxhdpi"/>
	</platform>
	<platform name="ios">
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="www/res/ios/icon/180x180.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="www/res/ios/icon/60x60.png" width="60" height="60" />
        <icon src="www/res/ios/icon/120x120.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="www/res/ios/icon/76x76.png" width="76" height="76" />
        <icon src="www/res/ios/icon/152x152.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="www/res/ios/icon/40x40.png" width="40" height="40" />
        <icon src="www/res/ios/icon/80x80.png" width="80" height="80" />
        <!-- iOS 6.1 -->
        <!-- iPhone / iPod Touch -->
        <icon src="www/res/ios/icon/57x57.png" width="57" height="57" />
        <icon src="www/res/ios/icon/114x114.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="www/res/ios/icon/72x72.png" width="72" height="72" />
        <icon src="www/res/ios/icon/144x144.png" width="144" height="144" />
        <!-- iPad Pro -->
        <icon src="www/res/ios/icon/167x167.png" width="167" height="167" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="www/res/ios/icon/29x29.png" width="29" height="29" />
        <icon src="www/res/ios/icon/58x58.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="www/res/ios/icon/50x50.png" width="50" height="50" />
        <icon src="www/res/ios/icon/100x100.png" width="100" height="100" />
    </platform>
	<platform name="ios">
    <!-- There are two mechanisms for showing launch images.
      -- Legacy method (supports all devices except iPad Pro 12.9):
      -- Note: Images are determined by width and height. The following are supported -->
		<splash src="www/res/ios/screen/320x480.png" width="320" height="480"/>
		<splash src="www/res/ios/screen/640x960.png" width="640" height="960"/>
		<splash src="www/res/ios/screen/768x1024.png" width="768" height="1024"/>
		<splash src="www/res/ios/screen/1536x2048.png" width="1536" height="2048"/>
		<splash src="www/res/ios/screen/1024x768.png" width="1024" height="768"/>
		<splash src="www/res/ios/screen/2048x1536.png" width="2048" height="1536"/>
		<splash src="www/res/ios/screen/640x1136.png" width="640" height="1136"/>
		<splash src="www/res/ios/screen/750x1334.png" width="750" height="1334"/>
		<splash src="www/res/ios/screen/1242x2208.png" width="1242" height="2208"/>
		<splash src="www/res/ios/screen/2208x1242.png" width="2208" height="1242"/>
	</platform>
	<preference name="SplashScreenDelay" value="10000" />
    <content src="index.html" />
    <access origin="*" />
	<allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
	<allow-navigation href="*" />
</widget>
